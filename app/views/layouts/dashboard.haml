!!!
%html
  %head
    %meta{:content => "text/html; charset=UTF-8", "http-equiv" => "Content-Type"}/
    %title Luxparfum
    = stylesheet_link_tag    'application', 'admin/dashboard', media: 'all'
    = javascript_include_tag 'application'
    = csrf_meta_tags
  %body.skin-black
    = render 'admin/dashboard/header'
    .wrapper.row-offcanvas.row-offcanvas-left
      = render 'admin/dashboard/side_menu'
      %aside.right-side
        %section.content
          = yield
